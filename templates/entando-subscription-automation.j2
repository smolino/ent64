---
apiVersion: operators.coreos.com/v1
kind: OperatorGroup
metadata:
  name: {{namespaces}}
  namespace: {{namespaces}}
spec:
  targetNamespaces:
  - {{namespaces}}
---
apiVersion: operators.coreos.com/v1alpha1
kind: Subscription
metadata:
  name: entando-k8s-operator
  namespace: {{namespaces}}
spec:
  channel: "final"
  name: entando-k8s-operator
  source: {{ source }}
  sourceNamespace: openshift-marketplace
